{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier", "jsdoc"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "jsdoc/no-undefined-types": "warn",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ]
    }
}
